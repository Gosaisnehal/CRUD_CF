@model CRUD_CF.Country
    @{
        ViewBag.Title = "Default";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <h2>Choose a Default country </h2>

    <p>
        <div class="form-group">
            @Html.LabelFor(model => model.countryId, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownListFor(model => model.countryId, ViewBag.Countrylist as SelectList, "Select You Country", new { @class = "form-control" })
                @*@Html.DropdownlistFor(model => model.countryId, ViewBag.new { htmlAttributes = new { @class = "form-control" } })*@
                @Html.ValidationMessageFor(model => model.countryId, "", new { @class = "text-danger" })
            </div>
        </div>
    </p>
